{
  "name": "re-taro.dev",
  "description": "My personal homepage",
  "version": "1.1.0",
  "packageManager": "pnpm@8.10.0",
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://re-taro.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/re-taro.dev"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "NODE_ENV=development remix dev --manual -c \"pnpm dev:wrangler\"",
    "dev:wrangler": "wrangler pages dev ./public",
    "build": "remix build",
    "start": "NODE_ENV=production wrangler pages dev ./public",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . -c",
    "lint:markuplint": "markuplint './**/*.tsx'",
    "lint:tsc": "tsc -p . --noEmit",
    "fmt": "run-s -c fmt:*",
    "fmt:eslint": "pnpm run lint:eslint --fix",
    "fmt:prettier": "prettier . -w",
    "gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.3.1",
    "@remix-run/cloudflare-pages": "2.3.1",
    "@remix-run/css-bundle": "2.3.1",
    "@remix-run/react": "2.3.1",
    "isbot": "3.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@cloudflare/workers-types": "4.20231121.0",
    "@markuplint/jsx-parser": "3.11.0",
    "@markuplint/ml-config": "3.13.0",
    "@markuplint/react-spec": "3.12.0",
    "@re-taro/eslint-config": "2.2.0",
    "@remix-run/dev": "2.3.1",
    "@types/node": "20.9.4",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@vanilla-extract/css": "1.14.0",
    "better-sqlite3": "9.1.1",
    "drizzle-kit": "0.20.4",
    "drizzle-orm": "0.29.0",
    "eslint": "8.54.0",
    "markuplint": "3.14.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "typescript": "5.3.2",
    "wrangler": "3.17.0"
  }
}
