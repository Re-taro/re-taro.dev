{
	"name": "re-taro.dev",
	"type": "module",
	"version": "1.1.0",
	"private": true,
	"packageManager": "pnpm@8.15.6",
	"description": "My personal homepage",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/re-taro.dev"
	},
	"sideEffects": false,
	"scripts": {
		"analyze": "remix vite:build --mode analyze && serve build/client",
		"build": "remix vite:build",
		"build-storybook": "storybook build",
		"dev": "remix vite:dev",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"gen:component": "plop",
		"gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts",
		"gen:css": "panda codegen",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -p . --noEmit",
		"prepare": "panda codegen",
		"start": "NODE_ENV=production wrangler pages dev build/client",
		"storybook": "storybook dev -p 6006",
		"test": "run-p -c test:*",
		"test:integration": "playwright test",
		"test:storybook": "test-storybook",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@remix-run/cloudflare": "2.8.1",
		"@remix-run/cloudflare-pages": "2.8.1",
		"@remix-run/react": "2.8.1",
		"isbot": "5.1.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "5.0.1"
	},
	"devDependencies": {
		"@axe-core/playwright": "4.9.0",
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.1",
		"@cloudflare/workers-types": "4.20240404.0",
		"@pandacss/dev": "0.37.2",
		"@pandacss/studio": "0.37.2",
		"@playwright/test": "1.42.1",
		"@re-taro/eslint-config": "4.0.0",
		"@remix-run/dev": "2.8.1",
		"@remix-run/node": "2.8.1",
		"@remix-run/testing": "2.8.1",
		"@storybook/addon-a11y": "8.0.6",
		"@storybook/addon-actions": "8.0.6",
		"@storybook/addon-backgrounds": "8.0.6",
		"@storybook/addon-essentials": "8.0.6",
		"@storybook/addon-interactions": "8.0.6",
		"@storybook/addon-links": "8.0.6",
		"@storybook/blocks": "8.0.6",
		"@storybook/react": "8.0.6",
		"@storybook/react-vite": "8.0.6",
		"@storybook/test": "8.0.6",
		"@storybook/test-runner": "0.17.0",
		"@testing-library/jest-dom": "6.4.2",
		"@testing-library/react": "14.2.2",
		"@testing-library/user-event": "14.5.2",
		"@types/node": "20.12.4",
		"@types/react": "18.2.74",
		"@types/react-dom": "18.2.24",
		"@vitejs/plugin-react-swc": "3.6.0",
		"axe-html-reporter": "2.2.3",
		"axe-playwright": "2.0.1",
		"better-sqlite3": "9.4.5",
		"concurrently": "8.2.2",
		"drizzle-kit": "0.20.14",
		"drizzle-orm": "0.30.7",
		"eslint": "8.57.0",
		"eslint-plugin-format": "0.1.0",
		"jsdom": "24.0.0",
		"npm-run-all2": "6.1.2",
		"playwright": "1.42.1",
		"plop": "4.0.1",
		"postcss": "8.4.38",
		"postcss-load-config": "5.0.3",
		"remix-development-tools": "4.1.4",
		"rollup-plugin-visualizer": "5.12.0",
		"serve": "14.2.1",
		"sirv-cli": "2.0.2",
		"storybook": "8.0.5",
		"typescript": "5.4.4",
		"vite": "5.2.8",
		"vite-tsconfig-paths": "4.3.2",
		"vitest": "1.4.0",
		"wait-on": "7.2.0",
		"wrangler": "3.45.0"
	}
}
