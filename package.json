{
	"name": "re-taro.dev",
	"type": "module",
	"version": "1.1.0",
	"private": true,
	"packageManager": "pnpm@8.15.5",
	"description": "My personal homepage",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/re-taro.dev"
	},
	"sideEffects": [
		"./app/styles/globals.css.ts"
	],
	"scripts": {
		"analyze": "remix vite:build --mode analyze && serve build/client",
		"build": "remix vite:build",
		"build-storybook": "storybook build",
		"dev": "remix vite:dev",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:markuplint": "markuplint './**/*.tsx'",
		"lint:tsc": "tsc -p . --noEmit",
		"start": "NODE_ENV=production wrangler pages dev build/client",
		"storybook": "storybook dev -p 6006",
		"test": "run-p -c test:*",
		"test:storybook": "test-storybook"
	},
	"dependencies": {
		"@remix-run/cloudflare": "2.8.1",
		"@remix-run/cloudflare-pages": "2.8.1",
		"@remix-run/react": "2.8.1",
		"isbot": "5.1.2",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.1",
		"@cloudflare/workers-types": "4.20240320.1",
		"@markuplint/jsx-parser": "4.4.0",
		"@markuplint/ml-config": "4.3.0",
		"@markuplint/react-spec": "4.2.0",
		"@re-taro/eslint-config": "4.0.0",
		"@remix-run/dev": "2.8.1",
		"@remix-run/node": "2.8.1",
		"@storybook/addon-a11y": "8.0.5",
		"@storybook/addon-actions": "8.0.5",
		"@storybook/addon-essentials": "8.0.5",
		"@storybook/addon-interactions": "8.0.5",
		"@storybook/addon-links": "8.0.4",
		"@storybook/blocks": "8.0.4",
		"@storybook/react": "8.0.4",
		"@storybook/react-vite": "8.0.4",
		"@storybook/test": "8.0.4",
		"@storybook/test-runner": "0.17.0",
		"@types/node": "20.11.30",
		"@types/react": "18.2.73",
		"@types/react-dom": "18.2.22",
		"@vanilla-extract/css": "1.14.1",
		"@vanilla-extract/vite-plugin": "4.0.6",
		"axe-playwright": "2.0.1",
		"better-sqlite3": "9.4.3",
		"drizzle-kit": "0.20.14",
		"drizzle-orm": "0.30.5",
		"eslint": "8.57.0",
		"eslint-plugin-format": "0.1.0",
		"markuplint": "4.4.0",
		"npm-run-all2": "6.1.2",
		"rollup-plugin-visualizer": "5.12.0",
		"serve": "14.2.1",
		"storybook": "8.0.4",
		"typescript": "5.4.3",
		"vite": "5.2.6",
		"vite-tsconfig-paths": "4.3.2",
		"wrangler": "3.39.0"
	}
}
