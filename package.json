{
	"name": "re-taro.dev",
	"version": "1.1.0",
	"packageManager": "pnpm@8.14.0",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"description": "My personal homepage",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/re-taro.dev"
	},
	"license": "MIT",
	"sideEffects": false,
	"scripts": {
		"build": "remix build",
		"dev": "NODE_ENV=development remix dev --manual -c \"pnpm dev:wrangler\"",
		"dev:wrangler": "wrangler pages dev ./public",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"fmt:prettier": "prettier . -w",
		"gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:markuplint": "markuplint './**/*.tsx'",
		"lint:prettier": "prettier . -c",
		"lint:tsc": "tsc -p . --noEmit",
		"start": "NODE_ENV=production wrangler pages dev ./public"
	},
	"dependencies": {
		"@remix-run/cloudflare": "2.5.1",
		"@remix-run/cloudflare-pages": "2.5.1",
		"@remix-run/css-bundle": "2.5.1",
		"@remix-run/react": "2.5.1",
		"hono": "3.12.8",
		"isbot": "4.4.0",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.1",
		"@cloudflare/workers-types": "4.20240117.0",
		"@markuplint/jsx-parser": "3.12.0",
		"@markuplint/ml-config": "3.14.0",
		"@markuplint/react-spec": "3.13.0",
		"@re-taro/eslint-config": "2.5.0",
		"@re-taro/prettier-config": "1.2.0",
		"@remix-run/dev": "2.5.1",
		"@types/node": "20.11.10",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"@vanilla-extract/css": "1.14.1",
		"better-sqlite3": "9.3.0",
		"drizzle-kit": "0.20.13",
		"drizzle-orm": "0.29.3",
		"eslint": "8.56.0",
		"markuplint": "3.15.0",
		"npm-run-all2": "6.1.1",
		"prettier": "3.2.4",
		"typescript": "5.3.3",
		"wrangler": "3.25.0"
	}
}
