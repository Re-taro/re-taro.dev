{
	"name": "re-taro.dev",
	"version": "1.1.0",
	"packageManager": "pnpm@8.15.3",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"type": "module",
	"description": "My personal homepage",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/re-taro.dev"
	},
	"license": "MIT",
	"sideEffects": [
		"./app/styles/globals.css.ts"
	],
	"scripts": {
		"analyze": "remix vite:build --mode analyze && serve build/client",
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"fmt:prettier": "prettier . -w",
		"gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:markuplint": "markuplint './**/*.tsx'",
		"lint:prettier": "prettier . -c",
		"lint:tsc": "tsc -p . --noEmit",
		"start": "NODE_ENV=production wrangler pages dev ./build/client"
	},
	"dependencies": {
		"@remix-run/cloudflare": "2.8.0",
		"@remix-run/cloudflare-pages": "2.8.0",
		"@remix-run/react": "2.8.0",
		"isbot": "5.1.1",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.1",
		"@cloudflare/workers-types": "4.20240222.0",
		"@markuplint/jsx-parser": "4.2.0",
		"@markuplint/ml-config": "4.2.0",
		"@markuplint/react-spec": "4.1.0",
		"@re-taro/eslint-config": "3.0.0",
		"@re-taro/prettier-config": "2.0.0",
		"@remix-run/dev": "2.8.0",
		"@remix-run/node": "2.8.0",
		"@types/node": "20.11.22",
		"@types/react": "18.2.60",
		"@types/react-dom": "18.2.19",
		"@vanilla-extract/css": "1.14.1",
		"@vanilla-extract/vite-plugin": "4.0.4",
		"better-sqlite3": "9.4.3",
		"drizzle-kit": "0.20.14",
		"drizzle-orm": "0.29.4",
		"eslint": "8.57.0",
		"markuplint": "4.2.0",
		"npm-run-all2": "6.1.2",
		"prettier": "3.2.5",
		"rollup-plugin-visualizer": "5.12.0",
		"serve": "14.2.1",
		"typescript": "5.3.3",
		"vite": "5.1.4",
		"vite-tsconfig-paths": "4.3.1",
		"wrangler": "3.30.0"
	}
}
