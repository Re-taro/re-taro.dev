{
  "name": "re-taro.dev",
  "description": "My personal homepage",
  "version": "1.1.0",
  "packageManager": "pnpm@8.6.12",
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://re-taro.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/re-taro.dev"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all build --parallel dev:*",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public",
    "build": "remix build",
    "start": "NODE_ENV=production wrangler pages dev ./public",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint . --ext .js,.cjs,.ts,.tsx,.json",
    "lint:prettier": "prettier --check './*.config.js' './**/*.{cjs,ts,tsx,json}'",
    "lint:markuplint": "markuplint .",
    "lint:tsc": "tsc -p . --noEmit",
    "fmt": "run-s -c fmt:*",
    "fmt:eslint": "pnpm run lint:eslint --fix",
    "fmt:prettier": "prettier --write './*.config.js' './**/*.{cjs,ts,tsx,json}'",
    "fmt:markuplint": "pnpm run lint:markuplint --fix",
    "gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.0.1",
    "@remix-run/cloudflare-pages": "2.0.1",
    "@remix-run/css-bundle": "2.0.1",
    "@remix-run/react": "2.0.1",
    "isbot": "3.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@cloudflare/workers-types": "4.20230922.0",
    "@markuplint/jsx-parser": "3.10.0",
    "@markuplint/ml-config": "3.12.0",
    "@markuplint/react-spec": "3.11.0",
    "@re-taro/eslint-config": "1.10.3",
    "@remix-run/dev": "2.0.1",
    "@types/node": "20.7.0",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.7",
    "@vanilla-extract/css": "1.13.0",
    "better-sqlite3": "8.6.0",
    "drizzle-kit": "0.19.13",
    "drizzle-orm": "0.28.6",
    "eslint": "8.50.0",
    "markuplint": "3.13.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "wrangler": "3.10.0"
  }
}
