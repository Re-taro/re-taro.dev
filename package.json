{
	"name": "re-taro.dev",
	"type": "module",
	"version": "1.1.0",
	"private": true,
	"packageManager": "pnpm@9.5.0",
	"description": "My personal homepage",
	"author": "Rintaro Itokawa <me@re-taro.dev> (https://re-taro.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/re-taro/re-taro.dev"
	},
	"sideEffects": false,
	"scripts": {
		"analyze": "remix vite:build --mode analyze && serve build/client",
		"build": "remix vite:build",
		"build-storybook": "storybook build",
		"dev": "remix vite:dev",
		"fmt": "run-s -c fmt:*",
		"fmt:eslint": "pnpm run lint:eslint --fix",
		"gen:component": "plop",
		"gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts",
		"gen:css": "panda codegen",
		"lint": "run-p -c lint:*",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc -p . --noEmit",
		"prepare": "panda codegen",
		"start": "NODE_ENV=production wrangler pages dev build/client",
		"storybook": "storybook dev -p 6006",
		"test": "run-p -c test:*",
		"test:integration": "playwright test",
		"test:storybook": "test-storybook",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@remix-run/cloudflare": "2.10.2",
		"@remix-run/cloudflare-pages": "2.10.2",
		"@remix-run/react": "2.10.2",
		"isbot": "5.1.12",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"temporal-polyfill": "0.2.5"
	},
	"devDependencies": {
		"@axe-core/playwright": "4.9.1",
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.7",
		"@cloudflare/workers-types": "4.20240712.0",
		"@iconify-json/circum": "1.1.11",
		"@iconify-json/fa-solid": "1.1.8",
		"@iconify-json/ri": "1.1.21",
		"@iconify-json/simple-icons": "1.1.109",
		"@pandacss/dev": "0.42.0",
		"@pandacss/studio": "0.42.0",
		"@playwright/test": "1.45.1",
		"@re-taro/eslint-config": "4.1.0",
		"@remix-run/dev": "2.10.2",
		"@remix-run/node": "2.10.2",
		"@remix-run/testing": "2.10.2",
		"@storybook/addon-a11y": "8.2.2",
		"@storybook/addon-actions": "8.2.2",
		"@storybook/addon-backgrounds": "8.2.2",
		"@storybook/addon-essentials": "8.2.2",
		"@storybook/addon-interactions": "8.2.2",
		"@storybook/addon-links": "8.2.2",
		"@storybook/blocks": "8.2.2",
		"@storybook/manager-api": "8.2.2",
		"@storybook/react": "8.2.2",
		"@storybook/react-vite": "8.2.2",
		"@storybook/test": "8.2.2",
		"@storybook/test-runner": "0.19.0",
		"@storybook/theming": "8.2.2",
		"@svgr/core": "8.1.0",
		"@svgr/plugin-jsx": "8.1.0",
		"@testing-library/jest-dom": "6.4.6",
		"@testing-library/react": "16.0.0",
		"@testing-library/user-event": "14.5.2",
		"@types/node": "20.14.10",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@vitejs/plugin-react-swc": "3.7.0",
		"axe-playwright": "2.0.1",
		"better-sqlite3": "11.1.2",
		"browserslist-to-esbuild": "2.1.1",
		"concurrently": "8.2.2",
		"drizzle-kit": "0.23.0",
		"drizzle-orm": "0.32.0",
		"eslint": "8.57.0",
		"eslint-plugin-format": "0.1.2",
		"jsdom": "24.1.0",
		"npm-run-all2": "6.2.2",
		"playwright": "1.45.1",
		"plop": "4.0.1",
		"postcss": "8.4.39",
		"postcss-load-config": "6.0.1",
		"remix-development-tools": "4.2.2",
		"rollup-plugin-visualizer": "5.12.0",
		"serve": "14.2.3",
		"sirv-cli": "2.0.2",
		"storybook": "8.2.2",
		"terser": "5.31.2",
		"typescript": "5.5.3",
		"unplugin-icons": "0.19.0",
		"vite": "5.3.3",
		"vite-tsconfig-paths": "4.3.2",
		"vitest": "2.0.2",
		"wait-on": "7.2.0",
		"wrangler": "3.64.0"
	},
	"browserslist": [
		"defaults and > 0.3%"
	]
}
